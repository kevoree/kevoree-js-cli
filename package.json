    "kevoree-library": "next",
    "kevoree-resolvers": "next",
    "mkdirp": "^0.5.1",
    "ora": "^0.3.0",
    "rmrf": "^1.0.2",
    "tiny-conf": "^1.0.0",
    "tiny-conf-plugin-argv": "^1.0.0",
    "tiny-conf-plugin-file": "^1.0.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "eslint": "^3.9.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1"
  },
  "license": "LGPL-3.0",
  "keywords": [
