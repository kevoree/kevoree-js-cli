{
  "name": "kevoree-cli",
  "version": "5.7.16",
  "author": "Maxime Tricoire <max.tricoire@gmail.com>",
  "main": "./lib/NodeJSRuntime",
  "preferGlobal": true,
  "bin": {
    "kevoree": "./bin/kevoree"
  },
  "scripts": {
    "lint": "eslint lib bin test",
    "start": "node ./bin/kevoree"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "es6-promise": "^4.1.0",
    "inquirer": "^3.0.6",
    "kevoree-commons": "^3.0.0",
    "kevoree-const": "^1.0.1",
    "kevoree-core": "^9.0.6",
    "kevoree-kevscript": "^4.0.5",
    "kevoree-library": "^5.0.10",
    "kevoree-resolvers": "^4.0.5",
    "mkdirp": "^0.5.1",
    "ora": "^1.1.0",
    "rmrf": "^1.0.2",
    "tiny-conf": "^1.0.4",
    "tiny-conf-plugin-argv": "^1.0.1",
    "tiny-conf-plugin-file": "^1.0.1",
    "update-notifier": "^2.1.0",
    "valid-url": "^1.0.9",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-plugin-promise": "^3.5.0"
  },
  "license": "LGPL-3.0",
  "keywords": [
    "kevoree",
    "runtime",
    "platform",
    "kevoree-cli",
    "kevoreejs"
  ],
  "description": "Manage your Kevoree Node.js runtime from the command-line",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kevoree/kevoree-js-cli.git"
  },
  "bugs": {
    "url": "https://github.com/kevoree/kevoree-js-cli/issues"
  },
  "homepage": "https://github.com/kevoree/kevoree-js-cli",
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "eslint:recommended",
    "plugins": [
      "promise"
    ],
    "rules": {
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "ignore"
        }
      ],
      "no-octal": 0,
      "no-native": 0,
      "no-console": 0
    }
  }
}
